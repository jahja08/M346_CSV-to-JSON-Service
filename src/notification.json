{
  "LambdaFunctionConfigurations": [
    {
      "Id": "CsvToJsonTrigger",
      "LambdaFunctionArn": "arn:aws:lambda:us-east-1:806334824074:function:Csv2JsonFunction",
      "Events": [
        "s3:ObjectCreated:Put"
      ],
      "Filter": {
        "Key": {
          "FilterRules": [
            {
              "Name": "suffix",
              "Value": ".csv"
            }
          ]
        }
      }
    }
  ]
}